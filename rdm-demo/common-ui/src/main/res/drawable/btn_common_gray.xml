<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >

        <shape >
            <!-- 内部颜色 -->
            <solid
                android:color="#aa7B7B7B"/>
            <!-- 圆角的幅度 -->
            <corners
                android:radius="1dip" />
        </shape>
    </item>
    <item android:state_enabled="false" >

        <shape >
            <!-- 内部颜色 -->
            <solid
                android:color="#7B7B7B"/>
            <!-- 圆角的幅度 -->
            <corners
                android:radius="1dip" />
        </shape>
    </item>

    <item >
        <shape >
            <!-- 内部颜色 -->
            <solid
                android:color="#7B7B7B" />
            <!-- 圆角的幅度 -->
            <corners
                android:radius="1dip" />
        </shape>

    </item>



</selector>